{
  "env": {
    "browser": true
  },
  "globals": {
    "fetch": true
  },
  "rules": {
    // Errors.
    "block-scoped-var": 2,
    "comma-style": [2, "last"],
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-implied-eval": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-nested-ternary": 2,
    "no-reserved-keys": 2,
    "no-trailing-spaces": 2,
    "no-undefined": 2,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "semi": [2, "always"],
    "strict": 2,
    // Warnings.
    "no-undef": 1,
    "max-nested-callbacks": [1, 3],
    "space-infix-ops": 1,
    "space-in-parens": [1, "never"],
    "space-in-brackets": [1, "never"],
    "space-before-blocks": [1, "always"],
    "space-after-keywords": [1, "always", {"checkFunctionKeyword": true}],
    "key-spacing": [1, {"beforeColon": false, "afterColon": true}],
    "brace-style": [1, "stroustrup", {"allowSingleLine": true}],
    "space-return-throw-case": 1,
    "comma-spacing": 1,
    // Disabled.
    "camelcase": 0,
    "spaced-line-comment": 0,
    "consistent-return": 0,
    "dot-notation": 0,
    "new-cap": 0,
    "no-alert": 0,
    "no-new": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "quotes": 0,
    "no-console": 0
  }
}
